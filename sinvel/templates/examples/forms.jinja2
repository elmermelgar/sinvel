{% extends "../layout.jinja2" %}

{% block styles %}
        <link href="{{request.static_url('sinvel:static/vendors/bower_components/bootstrap-select/dist/css/bootstrap-select.css')}}" rel="stylesheet">
        <link href="{{request.static_url('sinvel:static/vendors/bower_components/nouislider/distribute/jquery.nouislider.min.css')}}" rel="stylesheet">
        <link href="{{request.static_url('sinvel:static/vendors/bower_components/summernote/dist/summernote.css')}}" rel="stylesheet">
        <link href="{{request.static_url('sinvel:static/vendors/bower_components/eonasdan-bootstrap-datetimepicker/build/css/bootstrap-datetimepicker.min.css')}}" rel="stylesheet">
        <link href="{{request.static_url('sinvel:static/vendors/farbtastic/farbtastic.css')}}" rel="stylesheet">
{#        <link href="{{request.static_url('sinvel:static/vendors/chosen_v1.4.2/chosen.min.css')}}" rel="stylesheet">#}
{% endblock styles %}

{% block content %}
    <div class="container">
                <header class="page-header">
                    <h3>Basic Form Elements</h3>
                </header>

                <!-- Text Input -->
                <div class="tile">
                    <div class="t-header">
                        <div class="th-title">Input Text <small>Text Inputs with different sizes by height and column.</small></div>
                    </div>
                    <div class="t-body tb-padding">
                        <p>Text Inputs with different sizes by height and column.</p>

                        <input class="form-control input-lg m-b-10" type="text" placeholder=".input-lg">
                        <input type="text" class="form-control m-b-10" placeholder="Default">
                        <input class="form-control input-sm m-b-10" type="text" placeholder=".input-sm">
                        <div class="row m-b-10">
                            <div class="col-lg-2">
                                <input type="text" class="form-control m-b-10" placeholder=".col-lg-2">
                            </div>
                            <div class="col-lg-3">
                                <input type="text" class="form-control m-b-10" placeholder=".col-lg-3">
                            </div>
                            <div class="col-lg-4">
                                <input type="text" class="form-control m-b-10" placeholder=".col-lg-4">
                            </div>
                            <div class="col-lg-5">
                                <input type="text" class="form-control m-b-10" placeholder=".col-lg-5">
                            </div>
                            <div class="col-lg-6">
                                <input type="text" class="form-control m-b-10" placeholder=".col-lg-6">
                            </div>
                            <div class="clearfix"></div>
                        </div>

                        <br/>

                        <div class="row">
                            <div class="col-sm-4">
                                <p class=" c-black">Input Focused</p>
                                <input type="text" class="form-control input-focused m-b-20" placeholder="This is Focused...">
                            </div>

                            <div class="col-sm-4">
                                <p class=" c-black">Disabled</p>
                                <input type="text" class="form-control m-b-20" placeholder="This is Disabled..." disabled>
                            </div>

                            <div class="col-sm-4">
                                <p class=" c-black">Read Only</p>
                                <input type="text" class="form-control m-b-20" placeholder="This is Readonly..." readonly>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Textarea -->
                <div class="tile">
                    <div class="t-header">
                        <div class="th-title">Textarea <small>Form control which supports multiple lines of text. Change 'rows' attribute as necessary.</small></div>
                    </div>

                    <div class="t-body tb-padding">
                        <p class=" c-black">Basic example</p>
                        <textarea class="form-control m-b-10" rows="3" placeholder="This is a default Textarea..."></textarea>

                        <br/>

                        <p class=" c-black">Textarea with auto-grow height</p>
                        <textarea class="form-control auto-size m-b-10" placeholder="This is an auto sizing Textarea..."></textarea>

                        <br/>

                        <p class=" c-black">Textarea Disabled</p>
                        <textarea class="form-control" rows="3" placeholder="This is disabled..." disabled></textarea>
                    </div>

                </div>

                <!-- Select -->
                <div class="tile">
                    <div class="t-header">
                        <div class="th-title">
                            Select <small>Use the default option, or add multiple to show multiple options at once</small>
                        </div>
                    </div>

                    <div class="t-body tb-padding">
                        <p class=" c-black">Basic example</p>
                        <select class="form-control m-b-10">
                            <option>Option 1</option>
                            <option>Option 2</option>
                            <option>Option 3</option>
                            <option>Option 4</option>
                            <option>Option 5</option>
                        </select>

                        <br/>

                        <p class=" c-black">Multi Select</p>
                        <select multiple="multiple" class="form-control m-b-10">
                            <option>Option 1</option>
                            <option>Option 2</option>
                            <option>Option 3</option>
                            <option>Option 4</option>
                            <option>Option 5</option>
                        </select>

                        <br/>

                        <p class=" c-black">Select Disabled</p>
                        <select class="form-control" disabled>
                            <option>Option 1</option>
                            <option>Option 2</option>
                            <option>Option 3</option>
                        </select>
                    </div>
                </div>

                <!-- Checkbox + Radio -->
                <div class="tile">
                    <div class="t-header">
                        <div class="th-title">Checkbox & Radio <small>Checkboxes are for selecting one or several options in a list, while radios are for selecting one option from many.</small></div>
                    </div>

                    <div class="tbody tb-padding">
                        <p class="m-b-20 c-black ">Default (stacked)</p>
                        <div class="checkbox cr-alt m-b-20">
                            <label>
                                <input type="checkbox" value="">
                                <i class="input-helper"></i>
                                Option one is this and that-be sure to include why it's great
                            </label>
                        </div>

                        <div class="checkbox disabled cr-alt m-b-20">
                            <label>
                                <input type="checkbox" value="" disabled>
                                <i class="input-helper"></i>
                                Option two is disabled
                            </label>
                        </div>

                        <div class="radio cr-alt m-b-20">
                            <label>
                                <input type="radio" name="sample" value="" checked>
                                <i class="input-helper"></i>
                                Option one is this and that-be sure to include why it's great
                            </label>
                        </div>
                        <div class="radio cr-alt m-b-20">
                            <label>
                                <input type="radio" name="sample" value="">
                                <i class="input-helper"></i>
                                Option two can be something else and selecting it will deselect option one
                            </label>
                        </div>
                        <div class="radio cr-alt disabled">
                            <label>
                                <input type="radio" value="" disabled>
                                <i class="input-helper"></i>
                                Option three is disabled
                            </label>
                        </div>

                        <br/>
                        <br/>

                        <p class="m-b-20">Inline checkboxes and radios - Use <code>.checkbox-inline</code> or <code>.radio-inline</code> classes on a series of checkboxes or radios for controls that appear on the same line.</p>

                        <label class="checkbox checkbox-inline cr-alt">
                            <input type="checkbox" id="inlineCheckbox1" value="option1">
                            <i class="input-helper"></i>
                            1
                        </label>
                        <label class="checkbox checkbox-inline cr-alt">
                            <input type="checkbox" id="inlineCheckbox2" value="option2">
                            <i class="input-helper"></i>
                            2
                        </label>
                        <label class="checkbox-inline cr-alt">
                            <input type="checkbox" id="inlineCheckbox3" value="option3">
                            <i class="input-helper"></i>
                            3
                        </label>

                        <br/><br/>

                        <label class="radio-inline cr-alt">
                            <input type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1">
                            <i class="input-helper"></i>
                            1
                        </label>
                        <label class="radio-inline cr-alt">
                            <input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2">
                            <i class="input-helper"></i>
                            2
                        </label>
                        <label class="radio-inline cr-alt">
                            <input type="radio" name="inlineRadioOptions" id="inlineRadio3" value="option3">
                            <i class="input-helper"></i>
                            3
                        </label>
                    </div>

                </div>
            </div>
    <div class="container">
                <div class="page-header">
                    <h3>Form Components</h3>
                </div>

                <div class="tile">
                    <div class="t-header">
                        <div class="th-title">Input Groups <small>Extend form controls by adding text or buttons before, after, or on both sides of any text-based inputs.</small></div>
                    </div>

                    <div class="t-body tb-padding">
                        <p class="c-black  m-b-5">Basic Example</p>
                        <small>Place one add-on or button on either side of an input. You may also place one on both sides of an input.</small>

                        <br/><br/>

                        <div class="row">
                            <div class="col-sm-4">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="zmdi zmdi-account"></i></span>
                                    <input type="text" class="form-control" placeholder="Full Name">
                                </div>

                                <br/>

                                <div class="input-group">
                                    <span class="input-group-addon"><i class="zmdi zmdi-local-phone"></i></span>
                                    <input type="text" class="form-control" placeholder="Contact Number">
                                </div>

                                <br/>

                                <div class="input-group">
                                    <span class="input-group-addon"><i class="zmdi zmdi-email"></i></span>
                                    <input type="text" class="form-control" placeholder="Email Address">
                                </div>

                                <br/>

                                <div class="input-group">
                                    <span class="input-group-addon"><i class="zmdi zmdi-pin"></i></span>
                                    <input type="text" class="form-control" placeholder="Home Address">
                                </div>
                            </div>

                            <div class="col-sm-4">
                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Landscape">
                                    <span class="input-group-addon last"><i class="zmdi zmdi-landscape"></i></span>
                                </div>

                                <br/>

                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Weather">
                                    <span class="input-group-addon last"><i class="zmdi zmdi-sun"></i></span>
                                </div>

                                <br/>

                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Flight">
                                    <span class="input-group-addon last"><i class="zmdi zmdi-airplane"></i></span>
                                </div>

                                <br/>

                                <div class="input-group">
                                    <input type="text" class="form-control" placeholder="Location">
                                    <span class="input-group-addon last"><i class="zmdi zmdi-my-location"></i></span>
                                </div>
                            </div>

                            <div class="col-sm-4">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="zmdi zmdi-arrow-missed"></i></span>
                                    <input type="text" class="form-control" placeholder="Internet">
                                    <span class="input-group-addon last"><i class="zmdi zmdi-globe"></i></span>
                                </div>

                                <br/>

                                <div class="input-group">
                                    <span class="input-group-addon"><i class="zmdi zmdi-money"></i></span>
                                    <input type="text" class="form-control" placeholder="Notifications">
                                    <span class="input-group-addon last"><i class="zmdi zmdi-plus-circle-o"></i></span>
                                </div>

                                <br/>

                                <div class="input-group">
                                    <span class="input-group-addon"><i class="zmdi zmdi-mail-send"></i></span>
                                    <input type="text" class="form-control" placeholder="Layers">
                                    <span class="input-group-addon last"><i class="zmdi zmdi-layers "></i></span>
                                </div>

                                <br/>

                                <div class="input-group">
                                    <span class="input-group-addon"><i class="zmdi zmdi-portable-wifi"></i></span>
                                    <input type="text" class="form-control" placeholder="Messages">
                                    <span class="input-group-addon last"><i class="zmdi zmdi-dialpad"></i></span>
                                </div>
                            </div>
                        </div>

                        <br/><br/>
                        <p class="c-black  m-b-5 m-t-20">Different Sizes</p>
                        <small>You may use the extra sizing classes to change size of the feilds.</small>

                        <br/><br/>

                        <div class="input-group input-group-lg">
                            <span class="input-group-addon"><i class="zmdi zmdi-sun"></i></span>
                            <input type="text" class="form-control input-lg" placeholder="Weather">
                        </div>

                        <br/>

                        <div class="input-group">
                            <span class="input-group-addon"><i class="zmdi zmdi-sun"></i></span>
                            <input type="text" class="form-control" placeholder="Weather">
                        </div>

                        <br/>

                        <div class="input-group input-group-sm">
                            <span class="input-group-addon"><i class="zmdi zmdi-sun"></i></span>
                            <input type="text" class="form-control input-sm" placeholder="Weather">
                        </div>
                    </div>

                    <br/>
                </div>

                <div class="tile">
                    <div class="t-header">
                        <div class="th-title">Input Mask <small>An inputmask helps the user with the input by ensuring a predefined format. This can be useful for dates, numerics, phone numbers etc...</small></div>
                    </div>

                    <div class="t-body tb-padding">
                        <div class="row">
                            <div class="col-sm-3 m-b-20">
                                <div class="form-group">
                                    <p class="c-black">Date</p>
                                    <input type="text" class="form-control input-mask" data-mask="00/00/0000" placeholder="eg: 23/05/2014">
                                </div>
                            </div>

                            <div class="col-sm-3 m-b-20">
                                <div class="form-group">
                                    <p class="c-black">Time</p>
                                    <input type="text" class="form-control input-mask" data-mask="00:00:00" placeholder="eg: 23:06:55">
                                </div>
                            </div>

                            <div class="col-sm-3 m-b-20">
                                <div class="form-group">
                                    <p class="c-black">Date Time</p>
                                    <input type="text" class="form-control input-mask" data-mask="00/00/0000 00:00:00" placeholder="eg: 00/00/0000 00:00:00">
                                </div>
                            </div>

                            <div class="col-sm-3 m-b-20">
                                <div class="form-group">
                                    <p class="c-black">CEP</p>
                                    <input type="text" class="form-control input-mask" data-mask="00000-000" placeholder="eg: 00000-000">
                                </div>
                            </div>

                            <div class="col-sm-3 m-b-20">
                                <div class="form-group">
                                    <p class="c-black">Phone Number</p>
                                    <input type="text" class="form-control input-mask" data-mask="000-00-0000000" placeholder="eg: 000-00-0000000">
                                </div>
                            </div>

                            <div class="col-sm-3 m-b-20">
                                <div class="form-group">
                                    <p class="c-black">Phone with Odd</p>
                                    <input type="text" class="form-control input-mask" data-mask="(00) 0000-0000" placeholder="eg: (00) 0000-0000">
                                </div>
                            </div>

                            <div class="col-sm-3 m-b-20">
                                <div class="form-group">
                                    <p class="c-black">US Phone Number</p>
                                    <input type="text" class="form-control input-mask" data-mask="(000) 000-0000" placeholder="eg: (000) 000-0000">
                                </div>
                            </div>

                            <div class="col-sm-3 m-b-20">
                                <div class="form-group">
                                    <p class="c-black">CPF</p>
                                    <input type="text" class="form-control input-mask" data-mask="000.000.000-00" placeholder="eg: 000.000.000-00">
                                </div>
                            </div>

                            <div class="col-sm-3 m-b-20">
                                <div class="form-group">
                                    <p class="c-black">Money</p>
                                    <input type="text" class="form-control input-mask" data-mask="000.000.000.000.000,00" placeholder="eg: 000.000.000.000.000,00">
                                </div>
                            </div>

                            <div class="col-sm-3 m-b-20">
                                <div class="form-group">
                                    <p class="c-black">IP Address</p>
                                    <input type="text" class="form-control input-mask" data-mask="000.000.000.000" placeholder="eg: 000.000.000.000">
                                </div>
                            </div>

                            <div class="col-sm-3 m-b-20">
                                <div class="form-group">
                                    <p class="c-black">Percentage</p>
                                    <input type="text" class="form-control input-mask" data-mask="00000,00%" placeholder="eg: 00000,00%">
                                </div>
                            </div>

                            <div class="col-sm-3">
                                <div class="form-group">
                                    <p class="c-black">Credit Card</p>
                                    <input type="text" class="form-control input-mask" data-mask="0000 0000 0000 0000" placeholder="eg: 0000 0000 0000 0000">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="tile">
                    <div class="t-header">
                        <div class="th-title">Custom Select <small>A custom select for Bootstrap using it's Dropdown component</small></div>
                    </div>

                    <div class="t-body tb-padding">
                        <div class="row">
                            <div class="col-sm-3 m-b-25">
                                <p class=" m-b-15 c-black">Basic Example</p>

                                <select class="selectpicker">
                                    <option>Mustard</option>
                                    <option>Ketchup</option>
                                    <option>Relish</option>
                                </select>
                            </div>

                            <div class="col-sm-3 m-b-25">
                                <p class=" m-b-15 c-black">Serach Option</p>

                                <select class="selectpicker" data-live-search="true">
                                    <option>Mustard</option>
                                    <option>Ketchup</option>
                                    <option>Relish</option>
                                    <option>Tent</option>
                                    <option>Flashlight</option>
                                    <option>Toilet Paper</option>
                                </select>
                            </div>

                            <div class="col-sm-3 m-b-25">
                                <p class=" m-b-15 c-black">Option Groups</p>

                                <select class="selectpicker">
                                    <optgroup label="Picnic">
                                        <option>Mustard</option>
                                        <option>Ketchup</option>
                                        <option>Relish</option>
                                    </optgroup>
                                    <optgroup label="Camping">
                                        <option>Tent</option>
                                        <option>Flashlight</option>
                                        <option>Toilet Paper</option>
                                    </optgroup>
                                </select>
                            </div>

                            <div class="col-sm-3 m-b-25">
                                <p class=" c-black m-b-15">Disabled Options</p>

                                <select class="selectpicker">
                                    <option disabled="disabled">Mustard</option>
                                    <option>Ketchup</option>
                                    <option>Mustard</option>
                                    <option disabled="disabled">Ketchup</option>
                                    <option>Relish</option>
                                </select>
                            </div>
                        </div>

                        <div class="row m-t-20">

                            <div class="col-sm-3 m-b-25">
                                <p class=" c-black m-b-15">Multi Select</p>

                                <select class="selectpicker" multiple>
                                    <option>Mustard</option>
                                    <option>Ketchup</option>
                                    <option>Relish</option>
                                    <option>Toasted</option>
                                </select>
                            </div>

                            <div class="col-sm-3 m-b-25">
                                <p class=" c-black m-b-15">Limit Selection - Set to 2</p>

                                <select class="selectpicker"  multiple data-max-options="2" title='Choose some of...'>
                                    <option>Mustard</option>
                                    <option>Ketchup</option>
                                    <option>Relish</option>
                                    <option>Plain</option>
                                    <option>Steamed</option>
                                    <option>Toasted</option>
                                </select>
                            </div>

                            <div class="col-sm-3 m-b-25">
                                <p class=" c-black m-b-15">Display Count</p>

                                <select class="selectpicker" multiple data-selected-text-format="count">
                                    <option>Mustard</option>
                                    <option>Ketchup</option>
                                    <option>Relish</option>
                                </select>
                            </div>
                        </div>

                    </div>
                </div>

                <div class="tile">
                    <div class="t-header">
                        <div class="th-title">Chosen <small>Make long, unwieldy select boxes more user friendly.</small></div>
                    </div>

                    <div class="t-body tb-padding">
                        <div class="row">
                            <div class="col-sm-3 m-b-15">
                                <p class=" c-black m-b-15">Basic Example</p>
                                <select class="tag-select" data-placeholder="Choose a Country...">
                                    <option value="United States">United States</option>
                                    <option value="United Kingdom">United Kingdom</option>
